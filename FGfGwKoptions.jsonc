{
  //json5 allows for comments :)
  "center_text": false,
  "title": "This app is for goldfish who can't remember buttons. Are you a goldfish? :3c",
  "default_messages": [
    [
      "Welcome!"
    ],
    [
      "Feel free to resize the window to hide these lower lines."
    ],
    [
      "Also, press a non-numeric button to choose a keymap."
    ],
  ],
  "font_type": "Consolas",
  "font_size": 32,
  "font_margin": 4,
  "window_always_on_top": true,
  "icon_path": "pelleds.jpg",
  "window_width": 1000,
  "window_height": 400,
  "screen_width": 1920,
  "screen_height": 1080,
  "background_color": [
    //R,G,B
    20,
    20,
    20
  ],
  "text_color": [
    255,
    0,
    0
  ],
  "keymaps": {
    "GG:S Default": {
      "keys": {
        "W": "[↑]   Jump",
        "A": "[←]   Left",
        "S": "[↓]   Crouch",
        "D": "[→]   Right",
        //
        "U": "[P]   Punch",
        "I": "[S]   Slash",
        "J": "[K]   Kick",
        "K": "[HS]  HSlash",
        "O": "[D]   Dust"
      }
    },
    "GG:S HenryFBP Custom (E+R=RC,PB)": {
      "keys": { //individual keys
        "W": "[↑]   Jump",
        "A": "[←]   Left",
        "S": "[↓]   Crouch",
        "D": "[→]   Right",
        //
        "U": "[P]   Punch",
        "I": "[S]   Slash",
        "J": "[K]   Kick",
        "K": "[HS]  HSlash",
        "O": "[D]   Dust",
        // my custom keys below
        "E": "[RC]  Roman Cancel",
        "R": "[PB]  Psych Burst"
