      "chords": {
        //for testing key "chords", just taps
        "A S S": "you tapped [a s s]",
      },
      "combinations": { //key combos, hold CTRL + X
        "CTRL X": "Cut",
        "S K": "Crouching heavy slash"
      }
    },
    "Tekken 7 Default": {
      "keys": {
        "W": "[↑]   Jump",
        "A": "[←]   Left",
        "S": "[↓]   Crouch",
        "D": "[→]   Right",
